{
  "name": "robindrost/prismic-eloquent",
  "authors": [
    {
      "name": "Robin Drost",
      "email": "robindrost@gmail.com",
      "homepage": "https://github.com/robindrost"
    }
  ],
  "license": "MIT",
  "description": "Laravel package that let you use the Prismic api in a more (friendly) eloquent like way.",
  "require": {
    "php": "^7.1",
    "prismic/php-sdk": "5.0.0",
    "illuminate/support": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "^3.6",
    "orchestra/database": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "RobinDrost\\PrismicEloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RobinDrost\\PrismicEloquent\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RobinDrost\\PrismicEloquent\\Providers\\ServiceProvider"
      ]
    }
  }
}
